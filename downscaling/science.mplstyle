
figure.figsize: 7,7
figure.dpi : 100

lines.linewidth : 2
axes.linewidth: 1
font.family : serif
font.size : 20
axes.labelsize : 16
xtick.direction : in 
ytick.direction : in
xtick.top : True
ytick.right : True

xtick.major.size:    5    # major tick size in points
xtick.minor.size:    3       # minor tick size in points
xtick.major.width:   2     # major tick width in points
xtick.minor.width:   1     # minor tick width in points

ytick.major.size:    5     # major tick size in points
ytick.minor.size:    3       # minor tick size in points
ytick.major.width:   2     # major tick width in points
ytick.minor.width:   1     # minor tick width in points

xtick.major.pad : 5
xtick.minor.pad : 5
ytick.major.pad : 5
ytick.minor.pad : 5

xtick.labelsize : 16
ytick.labelsize : 16
legend.fontsize : 14
legend.frameon : False
text.usetex : False

savefig.bbox : tight
savefig.dpi : 100

axes.prop_cycle: cycler('color', ["black", "332288","88CCEE","44AA99","117733","999933","DDCC77","CC6677","882255","AA4499","brown","fd3c06","gray"])
